{
  "name": "react-dynamic-layout",
  "version": "0.0.0-development",
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.6",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "prop-types": "^15.5.8",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^0.3.0",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "files": [
    "build",
    "lib",
    "README.md"
  ],
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "webpack-dev-server --open --inline",
    "prebuild": "npm run test",
    "build:style-dark": "stylus ./src/style/dark/index.styl -o ./build/dark-theme.css -c",
    "build:style-base": "stylus ./src/style/base/index.styl -o ./build/base.css -c",
    "build:style": "npm run build:style-base && npm run build:style-dark",
    "build:style-move": "cp -R ./src/style ./lib/style",
    "build:webpack": "rimraf ./build && cross-env NODE_ENV=production webpack -p",
    "build:babel": "rimraf ./lib && babel src --out-dir lib",
    "build": "npm run build:webpack && npm run build:style && npm run build:babel && npm run build:style-move",
    "test": "karma start --single-run",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/albizures/react-dynamic-layout.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
